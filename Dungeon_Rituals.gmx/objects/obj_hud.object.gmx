with obj_player
if collision_line(x, y, x , y + 114, obj_wall, true, true)
    {
        //draw_circle(x, y, 25, false);
        draw_sprite_ext(sprite_index,-1,x,y-z,m,1,0,c_aqua,.5)
    }
if obj_player.talking = -1
{
with obj_help
    {
    if point_distance(x,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered(infotext , x , y - 64)
        }
    }

with obj_health_potion
    {
    if point_distance(x,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered("Pickup Health Potion" , x , y - 128)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
            instance_destroy()
            obj_player.vit += 10 + irandom(5)
            if obj_player.vit &gt; obj_player.maxvit
            obj_player.vit = obj_player.maxvit
            
            audio_play_sound(snd_potion_consume,0,0)
            }
        }
    }

with obj_mana_potion
    {
    if point_distance(x,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered("Pickup Mana Potion" , x , y - 128)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
            instance_destroy()
            obj_player.mana += 10 + irandom(5)
            if obj_player.mana &gt; obj_player.maxmana
            obj_player.mana = obj_player.maxmana
            
            audio_play_sound(snd_potion_consume,0,0)
            }
        }
    }

with obj_treasure_chest
    {
    if point_distance(x,y,obj_player.x,obj_player.y) &lt; 128
        {
        if image_index = 0
        infobox_centered("Open Chest" , x , y - 128)
        if image_index = 1
        infobox_centered("Close Chest" , x , y - 128)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
            audio_play_sound(snd_open_chest,0,0)
            image_index = !image_index
            
            if !opened
                {
                instance_create(x + irandom(128) - 64,y + 96,choose(obj_health_potion , obj_mana_potion))
                }
            
            opened = 1
            }
        }
    }

with obj_priest
    {
    if point_distance(x,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered("Talk to Priest" , x , y - 156)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
            //if dialogue_pos = 0
            //audio_play_sound(snd_priest_it_is_up_to_you,0,0)
            image_index = !image_index
            obj_player.talking = id
            if dialogue_pos = 3
            audio_play_sound(snd_priest_defeated,0,0)
            }
        }
    }
    

with obj_door_to_second_room
    {
    if point_distance(x + 64,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered("Enter Door" , x + 64 , y - 156)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
               
               room_goto_next()
                
            }
        }
    }
    

    
with obj_door_to_third_room
    {
    if point_distance(x - 64,y,obj_player.x,obj_player.y) &lt; 128
        {
             if(global.hasbook)
                {
                    infobox_centered("Enter Door" , x - 64 , y - 156)
                    if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
                    {
                           
                        room_goto_next()
                            
                            
                    }
                }   
                else
                {
                    infobox_centered("Must Obtain Summoner Book!" , x - 64 , y - 156)
                }
        }
    }



with obj_door_to_next_room
    {
    if point_distance(x - 64,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered("Enter Door" , x - 64 , y - 156)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
            room_goto_next()
            }
        }
    }

with obj_door_back
    {
    if point_distance(x + 64,y,obj_player.x,obj_player.y) &lt; 128
        {
        infobox_centered("Enter Door" , x + 64 , y - 156)
        if global.jpressed[0,3] || keyboard_check_pressed(ord("C")) || keyboard_check_pressed(ord("E"))
            {
            room_goto_previous()
            }
        }
    }

with obj_AI
if !dead
if collision_circle(x, y + 74, 74, obj_wall, true, true)
{
    draw_sprite_ext(spr_Spider1,-1,x,y-z,m,1,0,c_red,.5)
}


with obj_rangeAI
if !dead
if collision_circle(x, y + 74, 74, obj_wall, true, true)
{
    
    draw_sprite_ext(spr_Skeleton,-1,x,y-z,m,1,0,c_red,.5)
}
}
if obj_player.talking &gt; -1
    {
    with obj_player.talking
    scr_talking()
    }
    

